@model LandingPage.Models.ContributorViewModel

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="@Model.Content.FirstOrDefault(r => r.Key== "CeoKeywords").Value"/>
    <meta name="description" content="@Model.Content.FirstOrDefault(r => r.Key== "CeoDescription").Value"/>
    <title>@Model.Content.FirstOrDefault(r => r.Key== "CeoTitle").Value</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
</head>
<body>
    <div class="body-content">
        @RenderBody()
    </div>
</body>
</html>